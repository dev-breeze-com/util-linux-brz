msgstr " Tb.Khởi động     Đầu         Cuối  #cung từ Id  Hệ thống\n"

#: fdisks/sfdisk.c:940
#, c-format
msgid ""
"Units: blocks of 1024 bytes, counting from %d\n"
"\n"
msgstr ""
"Đơn vị: khối 1024 byte, đếm từ %d\n"
"\n"

